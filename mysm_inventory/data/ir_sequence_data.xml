<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">

        <record id="raw_material_sequence" model="ir.sequence">
            <field name="name">Raw Material Sequence</field>
            <field name="code">product.template.raw_material.sequence</field>
            <field name="prefix">RMM/%(y)s/</field>
            <field name="padding">5</field>
        </record>

        <record id="raw_materiral_client_sequence" model="ir.sequence">
            <field name="name">Raw Material Client Sequence</field>
            <field name="code">product.template.raw_material_client.sequence</field>
            <field name="prefix">RMC/%(y)s/</field>
            <field name="padding">5</field>
        </record>

        <record id="packing_pack_sequence" model="ir.sequence">
            <field name="name">Packing Pack Sequence</field>
            <field name="code">product.template.packaging.sequence</field>
            <field name="prefix">PPM/%(y)s/</field>
            <field name="padding">5</field>
        </record>

        <record id="packing_pack_client_sequence" model="ir.sequence">
            <field name="name">Packing Pack Client Sequence</field>
            <field name="code">product.template.packaging_client.sequence</field>
            <field name="prefix">PPC/%(y)s/</field>
            <field name="padding">5</field>
        </record>

        <record id="formulation_sequence" model="ir.sequence">
            <field name="name">Formulation Sequence</field>
            <field name="code">product.template.formulation.sequence</field>
            <field name="prefix">FB/%(y)s/</field>
            <field name="padding">5</field>
        </record>

        <record id="finished_product_sequence" model="ir.sequence">
            <field name="name">Finished Product Sequence</field>
            <field name="code">product.template.finished_product.sequence</field>
            <field name="prefix">FP/%(y)s/</field>
            <field name="padding">5</field>
        </record>
    </data>

</odoo>
