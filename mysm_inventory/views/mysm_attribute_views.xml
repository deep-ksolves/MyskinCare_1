<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="mysm_attribute_view_tree" model="ir.ui.view">
        <field name="name">mysm.attribute.view.tree</field>
        <field name="model">mysm.attribute</field>
        <field name="arch" type="xml">
            <tree string="Attribute" editable="top">
                <field name="name"/>
                <field name="field_name" optional="hide" groups="base.group_no_one"/>
            </tree>
        </field>
    </record>

    <record id="mysm_attribute_view_form" model="ir.ui.view">
        <field name="name">mysm.attribute.view.form</field>
        <field name="model">mysm.attribute</field>
        <field name="arch" type="xml">
            <form string="Attribute">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="field_name" groups="base.group_no_one"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="mysm_form_product_action" model="ir.actions.act_window">
        <field name="name">Form of Products</field>
        <field name="res_model">mysm.attribute</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[('field_name', '=', 'form.product')]</field>
        <field name="context">{'default_field_name': 'form.product'}</field>
    </record>

    <record id="mysm_raw_material_source_action" model="ir.actions.act_window">
