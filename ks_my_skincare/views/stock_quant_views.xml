<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="view_stock_quant_tree_inventory_editable_inherit_product_template" model="ir.ui.view">
            <field name="name">view.stock.quant.tree.inventory.editable.inherit.product.template</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree_inventory_editable"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='owner_id']" position="after">
                    <field name="supplier_id" groups="stock.group_tracking_owner" attrs="{'readonly': [('id', '!=', False)]}" options="{'no_create': True}"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>